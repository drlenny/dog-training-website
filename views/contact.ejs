<%- include('partials/header') %>

    </div>
    </section>

    <div class="container">
        <div class="row">
            <div class="col">
                <h3>Contact Me</h3>
                <form action="/contact" method="POST">
                    <div class="row">
                        <div class="col-md-6">
                            <div class="form-group">
                                <label for="emailAddress">Your email address</label>
                                <span class="requiredField">*</span>
                                <input class="form-control" placeholder="Your email address" name="emailAddress" type="email" id="emailAddress">
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="form-group">
                                <label for="emailAddress-confirm">Re-enter your email address</label>
                                <span class="requiredField">*</span>
                                <input class="form-control" placeholder="Re-enter your email address"
                                    name="emailAddress-confirm" type="email" id="emailAddress-confirm">
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-6">
                            <div class="form-group">
                                <label for="firstName">Your first name</label>
                                <span class="requiredField">*</span>
                                <input class="form-control" placeholder="Your first name" name="firstName" type="text" id="firstName">
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="form-group">
                                <label for="lastName">Your last name</label>
                                <span class="requiredField">*</span>
                                <input class="form-control" placeholder="Your last name"
                                    name="lastName" type="text" id="lastName">
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-6">
                            <div class="form-group">
                                <label for="phoneNumber">Your preferred phone number</label>
                                <span class="requiredField">*</span>
                                <input class="form-control" placeholder="Your preferred phone number" name="phoneNumber" type="text" id="phoneNumber">
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="form-group">
                                <label for="bestTime">What are the best times to call you?</label>
                                <input class="form-control" placeholder="What are the best times to call you?"
                                    name="bestTime" type="text" id="bestTime">
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-6">
                            <div class="form-group">
                                <label for="dogName">Your dog's name</label>
                                <span class="requiredField">*</span>
                                <input class="form-control" placeholder="Your dog's name" name="dogName" type="text" id="dogName">
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="form-group">
                                <label for="dogAge">Your dog's age</label>
                                <span class="requiredField">*</span>
                                <input class="form-control" placeholder="Your dog's age"
                                    name="dogAge" type="text" id="dogAge">
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-md-6">
                            <div class="form-group">
                                <label for="dogBreed">Your dog's breed</label>
                                <span class="requiredField">*</span>
                                <input class="form-control" placeholder="Your dog's breed" name="dogBreed" type="text" id="dogBreed">
                            </div>
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="message">How can I help?</label>
                        <span class="requiredField">*</span>
                        <textarea class="form-control" name="message" id="message" cols="30" rows="8" placeholder="Message"></textarea>
                    </div>
                    <div class="row">
                        <div class="col-md-12">
                            <button type="submit" class="btn btn-primary btn-icon  float-right">Submit</button>
                            <button type="Reset" class="btn btn-primary btn-icon  float-right">Reset</button>
                        </div>
                    </div>
                </form>
            </div>

        </div>

    </div>


    <%- include('partials/footer') %>